<script language="javascript">
    var selection = "전체";
    var queryString = "";
    function Enter_Check(){
        if(event.keyCode == 13){
            buttonClicked();  // 실행할 이벤트
        }
    }

    function buttonClicked() {
        var keyword = document.getElementById("keywordtext").value;
        var dateFrom = document.getElementById("directFrom").value;
        var dateUntil = document.getElementById("directUntil").value;
        var radio = document.getElementById("radio").value;
        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth()+1; //January is 0!
        var yyyy = today.getFullYear();
        $("input[name=term]:checked").each(function () {
            if($(this).val() == "1일"){
                console.log($(this).val());
                if(dd<10) {
                    dd='0'+dd
                }
                if(mm<10) {
                    mm='0'+mm
                }
                dateUntil = yyyy + "-" + mm + "-" + dd;
                dd = dd - 1;
                if(dd<10) {
                    dd='0'+dd
                }
                dateFrom = yyyy + "-" + mm + "-" + dd;
            }else if($(this).val() == "1주"){
                console.log($(this).val());
                if(dd<10) {
                    dd='0'+dd
                }
                if(mm<10) {
                    mm='0'+mm
                }
                dateUntil = yyyy + "-" + mm + "-" + dd;
                dd = dd - 7;
                if(dd<10) {
                    dd='0'+dd
                }
                dateFrom = yyyy + "-" + mm + "-" + dd;
            }else if($(this).val() == "1개월"){
                console.log($(this).val());
                if(dd<10) {
                    dd='0'+dd
                }
                if(mm<10) {
                    mm='0'+mm
                }
                dateUntil = yyyy + "-" + mm + "-" + dd;
                mm = mm - 1;
                if(mm<10) {
                    mm='0'+mm
                }
                dateFrom = yyyy + "-" + mm + "-" + dd;
            }else if($(this).val() == "3개월"){
                console.log($(this).val());
                if(dd<10) {
                    dd='0'+dd
                }
                if(mm<10) {
                    mm='0'+mm
                }
                dateUntil = yyyy + "-" + mm + "-" + dd;
                mm = mm - 3;
                if(mm<10) {
                    mm='0'+mm
                }
                dateFrom = yyyy + "-" + mm + "-" + dd;
            }else if($(this).val() == "6개월"){
                console.log($(this).val());
                if(dd<10) {
                    dd='0'+dd
                }
                if(mm<10) {
                    mm='0'+mm
                }
                dateUntil = yyyy + "-" + mm + "-" + dd;
                if(mm<6) {
                    mm = 11;
                    yyyy = yyyy - 1;
                }else{
                    mm = mm - 6;
                    if(mm<6) {
                        mm='0'+mm
                    }
                }
                dateFrom = yyyy + "-" + mm + "-" + dd;
            }else if($(this).val() == "1년"){
                console.log($(this).val());
                if(dd<10) {
                    dd='0'+dd
                }
                if(mm<10) {
                    mm='0'+mm
                }
                dateUntil = yyyy + "-" + mm + "-" + dd;
                yyyy = yyyy - 1;
                dateFrom = yyyy + "-" + mm + "-" + dd;
            }
        });


        $("input[name=sort]:checked").each(function () {
            queryString = queryString + "category=" + $(this).val() + "&";
            console.log(queryString);
        });
        $("input[name=paper]:checked").each(function () {
            queryString = queryString + "provider=" + $(this).val() + "&";
            console.log(queryString);
        });
        if(dateFrom)
            queryString = queryString + "datefrom=" + dateFrom + "&";
        if(dateUntil)
            queryString = queryString + "dateuntil=" + dateUntil;

        if (!keyword) {
            {window.location.href = "/year-keyword" ;}
        }else {
            {window.location.href = "/search?query=" + keyword + "&" + queryString;}
        }
    }
</script>


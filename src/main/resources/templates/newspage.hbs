<style>
    .columns{
        columns: 3;
    }

    #floating-bar {
        height: 220px;
        width: 80px;
        position: fixed;
        right: 3%;
        top: 35%;
        margin: 0 0 0 110px;
        padding: 5px;
    }
    #essaytitle{
        position: relative;
        background: #fff;
        text-align: center;
    }
    #essaytitle > .container {
        padding: 7em 0 3em 0;
        border-bottom: solid 2px #e5e5e5;
        box-shadow: inset 0px -8px 0px 0px #fff, inset 0px -10px 0px 0px #e5e5e5;
        text-align: center;
    }

    #essaytitle h3 {
        font-weight: 700;
        color: #ed786a;
        text-shadow: 0.05em 0.075em 0 rgba(0, 0, 0, 0.1);
        letter-spacing: 13px;
    }
</style>
<body class="no-sidebar is-preload">
<div id="page-wrapper">

    <!-- Header -->
    <section id="essaytitle">
        <div class="container">
            <!-- Logo -->
            <h3 style="word-break: keep-all">{{title}}</h3>
            <p style="text-align: center; margin-bottom: 2px">
                {{#if category0}} {{category0}} {{/if}}
                {{#if category1}} / {{category1}} {{/if}}
                {{#if category2}} / {{category2}} {{/if}}
                {{#if category3}} / {{category3}} {{/if}}
                {{#if category4}} / {{category4}} {{/if}}
                {{#if category5}} / {{category5}} {{/if}}
                {{#if category6}} / {{category6}} {{/if}}
                {{#if category7}} / {{category7}} {{/if}}
                {{#if category8}} / {{category8}} {{/if}}
